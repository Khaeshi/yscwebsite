{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "astro.config.ts",
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "compilerOptions": {
      "target": "ESNext",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "baseUrl": ".",
      "skipLibCheck": true,
      "jsx": "react-jsx",
      "jsxImportSource": "react",
      "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      }
    },
}